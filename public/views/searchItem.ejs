<div class="col-md-3">
    <div class="card my-3 gif-card">
        <img class="card-img-top" width="400" height="200" src="<%= searchItem[1].images.fixed_height.url %>" alt="">
        <div class="card-body">
            <h4 class="card-title">                 
                <% if(searchItem[1].username == '') { %>
                    <%= 'Unknown' %>
                <% } else { %> 
                    <%= searchItem[1].username %>
                <% } %>
            </h4>
            <p class="card-text"><%= searchItem[1].title %></p>
        </div>
    </div>
</div>
